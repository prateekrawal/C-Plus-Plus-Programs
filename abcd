                //--------------------------------------------------------------  Document Ready Start  -------------------------------------------------------
                $(document).ready(function() {
                    
                    //Timeline.
                    $Lightning.use("HealthCloudGA:HcTimelineApp", function() {
                        $Lightning.createComponent("HealthCloudGA:HcPatientTimeline", {
                            patientId: "{!Contact.Id}",
                            shownInAlohaConsole: true
                        }, "lightningout-timeline");
                    });
                    
                    createChart('#salesPieChart1', objSalesPichartOptions1);
                    createChart('#salesPieChart2', objSalesPichartOptions2);
                    createChart('#salesPieChart3', objSalesPichartOptions3);
                    
                    $("#btnScriptSection").click(function(){
                        if($("#helpScritSection").hasClass("slds-is-open")) {
                            $("#helpScritSection").removeClass("slds-is-open");
                        } else {
                            $("#helpScritSection").addClass("slds-is-open");
                        }                       
                    });
                    $("#btnScriptSection").click(function(){
                        if($("#iconHelpScriptsShow").hasClass("hiddenElement")) {
                            $("#iconHelpScriptsShow").removeClass("hiddenElement");
                            $("#iconHelpScriptsHidden").addClass("hiddenElement");
                        } else {
                            $("#iconHelpScriptsShow").addClass("hiddenElement");
                            $("#iconHelpScriptsHidden").removeClass("hiddenElement");
                        }                       
                    });
                });

                //--------------------------------------------------------------  Document Ready End  -------------------------------------------------------
                
                //----------------------------------------------------------- General Function Start ---------------------------------------------------------
                /** 
                 * Function to display the appropriate content of the middle section according to the selected element.
                 * @param idContent The id of the content we want to display
                **/
                function displayContent(idContent) {
                    
                    // Weiterate through all the main elements of the middle section to search for the desired content
                    $('#mainContent > div').each(function(){
                        var childContainer = $(this);
                        
                        // If we find the correct element, we remove the class to hide it else, we hide the hide the element if needed.
                        if(childContainer.attr("id") === idContent ) {
                            childContainer.removeClass("hiddenElement");
                        } else if(!childContainer.hasClass("childContainer")) {
                            childContainer.addClass("hiddenElement");
                        }
                    });
                    
                    // Weiterate through all the main elements of the middle section to search for the desired content
                    $('#sidebarMenuOptions > div').each(function(){
                        var childContainer = $(this);
                        
                        // If we find the correct element, we remove the class to hide it else, we hide the hide the element if needed.
                        if(childContainer.attr("id") === idContent + "Menu" ) {
                            childContainer.removeClass("hiddenElement");
                        } else if(!childContainer.hasClass("childContainer")) {
                            childContainer.addClass("hiddenElement");
                        }
                    });
                    
                    $('#memberPickerSection > div').each(function(){
                        var childContainer = $(this);
                        
                        // If we find the correct element, we remove the class to hide it else, we hide the hide the element if needed.
                        if(childContainer.attr("id") === idContent + "Button" ) {
                            childContainer.removeClass("iconInactive");
                            childContainer.addClass("slds-icon-text-primary");
                        } else if(childContainer.hasClass("slds-icon-text-primary")) {
                            childContainer.addClass("iconInactive");
                            childContainer.removeClass("slds-icon-text-primary");
                        }
                    });
                    createChart('#carePieChart1', objCarePichartOptions1);
                    createChart('#carePieChart2', objCarePichartOptions2);
                    
                }
                
                /*
                 * Closes and open a section accordin to parameters.
                 * @param  strCloseComponentSelectors Selector of the container to close.
                 * @param  strOpenComponentSelectors Selector of the container to open.
                 */
                function closeSectionOpenSection(strCloseComponentSelectors, strOpenComponentSelectors) {
                    var arrSelectorsToClose;
                    var arrSelectorsToOpen;
                    var intCloseIndex = 0;
                    var intOpenIndex = 0;
                    if(stringIsNotBlank(strCloseComponentSelectors)) {
                        arrSelectorsToClose = strCloseComponentSelectors.split('&&&');
                    }
                    if(stringIsNotBlank(strOpenComponentSelectors)) {
                        arrSelectorsToOpen = strOpenComponentSelectors.split('&&&');
                    }
                    for(intCloseIndex in arrSelectorsToClose) {
                        if(!isNaN(intCloseIndex)) {
                            var objCloseComponent = getJQueryObject(arrSelectorsToClose[intCloseIndex]);
                            if(objCloseComponent != null) {
                                objCloseComponent.hide();
                            }
                        }
                    }
                    for(intOpenIndex in arrSelectorsToOpen) {
                        if(!isNaN(intOpenIndex)) {
                            var objOpenComponent = getJQueryObject(arrSelectorsToOpen[intOpenIndex]);
                            if(objOpenComponent != null) {
                                objOpenComponent.show();
                            }
                        }
                    }
                } 
                
                /*
                 * Shows/hides modal.
                 * @param Selector of the modal to close/open.
                 */
                function showHideModal(strSelector) {
                    if(stringIsNotBlank(strSelector)) {
                        var objModalComponent = getJQueryObject(strSelector);
                        var objLightningFullBackdrop = getJQueryObject("#lightningFullBackdrop");
                        if(objModalComponent != null) {
                            objModalComponent.toggleClass("slds-fade-in-open");
                            objLightningFullBackdrop.toggleClass("slds-backdrop_open");
                            objLightningFullBackdrop.toggleClass("slds-backdrop_close");
                        }
                    }
                }

                /*
                 * Shows/hides panel.
                 * @param Selector of the modal to close/open.
                 */
                function showHidePanel(strSelector) {
                    if(stringIsNotBlank(strSelector)) {
                        var objPanelComponent = getJQueryObject(strSelector);
                        if(objPanelComponent != null) {
                            objPanelComponent.toggleClass("slds-panel_docked");
                        }
                    }
                }
                
                /**
                ** Function to display the selected menu option section.
                ** @param menuSection the name of the menu section we need to look for
                ** @param contentSection The name of the content section to display.
                **/
                function toggleMenuOption(menuSection, contentSection) {
                    $('#' + menuSection + 'MenuOptions > div').each(function(){
                        var childContainer = $(this);
                        
                        // If we find the correct element, we remove the class to hide it else, we hide the hide the element if needed.
                        if(childContainer.attr("id") === contentSection + "Option" ) {
                            childContainer.removeClass("inactiveSidebarText");
                            childContainer.addClass("sidebarButtonActive");
                            childContainer.addClass("slds-text-color_inverse");
                        } else if(!childContainer.hasClass("childContainer")) {
                            childContainer.addClass("inactiveSidebarText");
                            childContainer.removeClass("sidebarButtonActive");
                            childContainer.removeClass("slds-text-color_inverse");
                        }
                    });
                    
                    $('#' + menuSection + 'Sections > div').each(function(){
                        var childContainer = $(this);
                        
                        // If we find the correct element, we remove the class to hide it else, we hide the hide the element if needed.
                        if(childContainer.attr("id") === contentSection + "Section" ) {
                            childContainer.removeClass("hiddenElement");
                        } else if(!childContainer.hasClass("childContainer")) {
                            childContainer.addClass("hiddenElement");
                        }
                    });
                }
            
                //----------------------------------------------------------- General Function End ---------------------------------------------------------
            </script>
